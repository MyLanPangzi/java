CMD /usr/local/apache-tomcat-9.0.27/bin/startup.sh && \
tail -F /usr/local/apache-tomcat-9.0.27/logs/catalina.out

docker run -itp 8081:8080 \
-v /root/tomcat/logs:/usr/local/apache-tomcat-9.0.27/logs \
-v /root/tomcat/webapps/:/usr/local/apache-tomcat-9.0.27/webapps/ \
--privileged=true \
--name mytomcat mytomcat
